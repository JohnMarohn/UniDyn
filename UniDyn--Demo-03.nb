            RowBox[{"f", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", 
            RowBox[{"f", "[", 
             RowBox[{"[", 
              RowBox[{"-", "1"}], "]"}], "]"}]}], "}"}], ",", 
          RowBox[{"{", 
